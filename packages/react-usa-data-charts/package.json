{
  "name": "@confrontend/react-usa-data-charts",
  "version": "1.0.6",
  "private": true,
  "dependencies": {
    "@confrontend/ui-components": "^1.0.1",
    "@emotion/react": "11.9.0",
    "@emotion/styled": "11.8.1",
    "@mui/material": "5.8.1",
    "papaparse": "5.3.2",
    "react-google-charts": "4.0.0",
    "sass": "1.51.0",
